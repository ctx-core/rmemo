{
	"name": "rmemo",
	"version": "0.29.1",
	"description": "ctx-core rmemo",
	"keywords": [
		"ctx-core",
		"rmemo",
		"reactive",
		"state management",
		"reactive state management",
		"memo",
		"reactive memo",
		"signal",
		"reactive signal"
	],
	"homepage": "https://github.com/ctx-core/rmemo#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/rmemo/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/rmemo.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build",
		"test": "pnpm run /^test:/",
		"test:size": "size-limit",
		"test:type": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"ctx-core": "^5.16.1"
	},
	"devDependencies": {
		"@size-limit/preset-small-lib": "^11.0.1",
		"c8": "^9.0.0",
		"check-dts": "^0.7.2",
		"size-limit": "^11.0.1",
		"tsx": "^4.7.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false,
	"size-limit": [
		{
			"name": "memo_",
			"import": {
				"": "{ memo_ }"
			},
			"limit": "355 B"
		},
		{
			"name": "memo_ sig_",
			"import": {
				"": "{ sig_, memo_ }"
			},
			"limit": "377 B"
		},
		{
			"name": "memo_ sig_ be_ ctx_",
			"import": {
				"": "{ sig_, memo_, be_, ctx_ }"
			},
			"limit": "478 B"
		},
		{
			"name": "memo_ sig_ be_ ctx_ be_memo_pair_ be_sig_triple_",
			"import": {
				"": "{ sig_, memo_, be_, ctx_, be_memo_pair_, be_sig_triple_ }"
			},
			"limit": "580 B"
		}
	]
}
